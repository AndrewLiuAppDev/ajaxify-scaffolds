<tr id="<%= dom_id(movie) %>">
        <td id="<%= dom_id(movie) %>_title"><%= link_to movie.title, edit_title_path(movie), remote: true %></td>
        <td><%= movie.description %></td>
        <td><%= movie.duration %></td>
        <td><%= movie.year %></td>
        <td><%= movie.director.name %></td>
        <td><%= link_to 'Edit', edit_movie_path(movie), remote: true %></td>
        <td><%= link_to 'Destroy', movie, method: :delete, remote: true %></td>
</tr>